{
    "name": "@cthunline/games",
    "version": "2.3.1",
    "license": "AGPL-3.0-or-later",
    "homepage": "https://doc.cthunline.org/",
    "repository": {
        "type": "git",
        "url": "https://github.com/cthunline/cthunline-games.git"
    },
    "scripts": {
        "build": "tsc --build --clean tsconfig.json && tsc --project tsconfig.json",
        "prepare": "[ -d '.husky' ] && husky install || true",
        "lint": "npm run lint:biome && npm run lint:ts",
        "format": "npm run format:biome && npm run lint:ts",
        "lint:biome": "npx @biomejs/biome check .",
        "format:biome": "npx @biomejs/biome check --apply .",
        "lint:ts": "tsc --noEmit"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@sinclair/typebox": "^0.32.15"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.5.3",
        "@tsconfig/recommended": "^1.0.3",
        "husky": "^9.0.11",
        "typescript": "^5.3.3"
    }
}
