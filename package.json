    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@sinclair/typebox": "^0.33.7"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.8.3",
        "@tsconfig/recommended": "^1.0.7",
        "husky": "^9.1.5",
        "typescript": "^5.5.4"
    }
}
